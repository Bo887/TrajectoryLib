import java.io.*;
public class Test{
	public static void main(String[] args) throws IOException{
		//start_vel, end_pos, end_vel, max_vel, accel, dt
		TrajectoryGenerator test = new TrajectoryGenerator(0,10,1,12,15,0.05);
		PrintWriter out = new PrintWriter(new FileWriter("testgenerator.dat"));	
		Segment[] traj = test.generateTraj();
		for(int i=0;i<traj.length;i++){
			System.out.printf("Pos: %f   Vel: %f   Accel: %f   Time: %f\n",traj[i].pos,traj[i].vel,traj[i].accel,traj[i].time);
		}
	}	
}	
