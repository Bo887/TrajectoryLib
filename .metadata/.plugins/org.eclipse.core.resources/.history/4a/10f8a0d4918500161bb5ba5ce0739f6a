import java.io.*;
public class Test{
	public static void main(String[] args) throws IOException{
		//start_vel, end_pos, end_vel, max_vel, accel, dt
		TrajectoryGenerator test = new TrajectoryGenerator(0,20,0,12,15,0.05);
		PrintWriter out = new PrintWriter("C:\\Users\\Eric\\Desktop\\generated_ff_values");	
		Segment[] traj = test.generateTraj();
		for(int i=0;i<traj.length;i++){
			printf("Total Dist: %f   Total Time: %f, Accel Time: %f   Time at end of Cruise: %f\n",test.end_pos,test.accel_time,(test.accel_time+test.cruise_time));
			out.printf("Pos:%f   Vel:%f   Accel:%f   Time:%f\r\n",traj[i].pos,traj[i].vel,traj[i].accel,traj[i].time);
			//out.println();
		}
		out.close();
	}	
}	
