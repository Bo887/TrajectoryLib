import java.io.*;
import java.util.ArrayList;
public class Test{
	public static void main(String[] args) throws IOException{
		//start_vel, end_pos, end_vel, max_vel, accel, dt
		TrajectoryGenerator test = new TrajectoryGenerator(0,20,0,12,15,0.05);
		//PrintWriter out = new PrintWriter("C:\\Users\\Eric\\Desktop\\generated_ff_values");	
		ArrayList<Segment> traj = test.generateTraj();
		for(int i=0;i<traj.size();i++){
			System.out.printf("Pos:%f   Vel:%f   Accel:%f   Time:%f\r\n",traj.get(i).pos,traj.get(i).vel,traj.get(i).accel,traj.get(i).time);
			//out.println();
		}
		//out.close();
	}	
}	
